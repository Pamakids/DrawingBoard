<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark" minHeight="40"
		maxHeight="150" minWidth="15" alpha.disabled="0.5">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

	<!-- states -->
	<s:states>
		<s:State name="disabled"/>
		<s:State name="down"/>
		<s:State name="over"/>
		<s:State name="up"/>
	</s:states>

	<s:BitmapImage id="up"
				   source="@Embed('assets/homepage/img/common/toParent_scroller.png', 
				   scaleGridTop=6,  
				   scaleGridLeft=1,  
				   scaleGridBottom=7,  
				   scaleGridRight=2)" left="0" right="0" bottom="0" top="0"/>

	<!--<s:Image source="{HomepageAssets.toParent_scroller}"/>-->

	<!-- SkinParts
		 name=iconDisplay, type=spark.primitives.BitmapImage, required=false
		 name=labelDisplay, type=spark.core.IDisplayText, required=false
	-->
</s:Skin>
